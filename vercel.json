{
    "version": 2,
    "builds": [
      {
        "src": "ai_portfolio/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "ai_portfolio/wsgi.py"
      }
    ],
    "env": {
      "SECRET_KEY": "@django_secret_key",
      
      "ALLOWED_HOSTS": ".vercel.app"
    }
  }
  